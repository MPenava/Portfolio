---
import CertificatesCarusel from "./CertificatesCarusel";

import vue from "@/assets/images/certs/vue.png";
import php from "@/assets/images/certs/php.png";
import phpoop from "@/assets/images/certs/php-oop.png";
import laravel from "@/assets/images/certs/laravel.png";
import golang from "@/assets/images/certs/golang.png";

const certs = [
  {
    id: "vue",
    image: vue.src,
  },
  {
    id: "php",
    image: php.src,
  },
  {
    id: "phpoop",
    image: phpoop.src,
  },
  {
    id: "laravel",
    image: laravel.src,
  },
  {
    id: "golang",
    image: golang.src,
  },
];
---

<CertificatesCarusel client:idle certificates={certs} />
